{
  "name": "bapcat/collection",
  "description": "Powerful but easy to use collections with support for lazy loading",
  "license" : "GPL-3.0-or-later",
  "homepage": "https://github.com/BapCat/Collection",
  "keywords": ["Collection", "Array", "Storage", "Read only", "Lazy"],
  "authors": [
    {
      "name": "Corey Frenette",
      "email": "corey@narwhunderful.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "7.4"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require": {
    "php" : "^7.4|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "BapCat\\Collection\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  }
}
